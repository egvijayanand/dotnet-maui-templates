<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Item">
	<TemplateData>
		<DefaultName>MauiView.xaml</DefaultName>
		<Name>Content View (.NET MAUI)</Name>
		<Description>A view for displaying content using XAML.</Description>
		<ProjectType>CSharp</ProjectType>
		<ProjectSubType>MAUI</ProjectSubType>
		<SortOrder>120</SortOrder>
		<Icon>__TemplateIcon.ico</Icon>
	</TemplateData>
	<TemplateContent>
		<CustomParameters>
			<CustomParameter Name="$xaml_item$" Value="" />
			<CustomParameter Name="$maui_global$" Value="" />
			<CustomParameter Name="$net8_or_later$" Value="" />
			<CustomParameter Name="$net10_or_later$" Value="" />
		</CustomParameters>
		<References />
		<ProjectItem TargetFileName="$fileinputname$View.xaml" ReplaceParameters="true">MauiView.xaml</ProjectItem>
		<ProjectItem TargetFileName="$fileinputname$View.xaml.cs" ReplaceParameters="true">MauiView.xaml.cs</ProjectItem>
	</TemplateContent>
	<WizardExtension>
		<Assembly>VijayAnand.MauiTemplates, Version=2.0.0.0, Culture=neutral, PublicKeyToken=7550554040ebdd44</Assembly>
		<FullClassName>VijayAnand.MauiTemplates.CustomActionWizard</FullClassName>
	</WizardExtension>
</VSTemplate>
