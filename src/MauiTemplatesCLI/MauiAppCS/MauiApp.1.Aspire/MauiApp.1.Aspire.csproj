<Project Sdk="Microsoft.NET.Sdk">

    <Sdk Name="Aspire.AppHost.Sdk" Version="$(AspireVersion)" />

    <PropertyGroup>
        <TargetFramework>MAUI_TFM</TargetFramework>
        <OutputType>Exe</OutputType>
        <!-- Until .NET MAUI is officially supported. -->
        <NoWarn>$(NoWarn);ASPIRE004</NoWarn>

        <!-- Project Options -->
        <Nullable>enable</Nullable>
        <!--#if (Net10OrLater)-->
        <LangVersion>preview</LangVersion>
        <!--#else-->
        <LangVersion>latest</LangVersion>
        <!--#endif-->
        <ImplicitUsings>enable</ImplicitUsings>
        <RootNamespace>MauiApp._1</RootNamespace>
    </PropertyGroup>
  
    <ItemGroup>
        <!--#if (CentralPkgMgmt)-->
        <PackageReference Include="Aspire.Hosting.AppHost" />
        <PackageReference Include="Aspire.Hosting.Maui" />
        <!--#else-->
        <PackageReference Include="Aspire.Hosting.AppHost" Version="$(AspireVersion)" />
        <PackageReference Include="Aspire.Hosting.Maui" Version="13.*-*" />
        <!--#endif-->
    </ItemGroup>
  
    <ItemGroup>
        <ProjectReference Include="..\SAFE-NAME\SAFE-NAME.csproj" />
    </ItemGroup>

</Project>
