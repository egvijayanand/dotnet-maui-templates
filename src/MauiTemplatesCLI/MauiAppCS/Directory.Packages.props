<Project>
    <PropertyGroup>
        <!--#if (CentralPkgMgmt)-->
        <ManagePackageVersionsCentrally>true</ManagePackageVersionsCentrally>
        <!--#if (AddAspire)-->
        <AspireVersion>13.1.0</AspireVersion>
        <!--#endif-->
        <!--#if (Net10)-->
        <!--#if (Nightly)-->
        <MauiVersion>10.0.40-ci.main.26065.2</MauiVersion>
        <!--#else-->
        <MauiVersion>10.0.30</MauiVersion>
        <!--#endif-->
        <!--#elif (Net9)-->
