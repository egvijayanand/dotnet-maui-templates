        <!--#if (Net9)-->
        <PackageVersion Include="CommunityToolkit.Maui.MediaElement" Version="6.1.3" />
        <!--#elif (Net8)-->
        <PackageVersion Include="CommunityToolkit.Maui.MediaElement" Version="4.1.2" />
        <!--#else-->
        <PackageVersion Include="CommunityToolkit.Maui.MediaElement" Version="7.0.0" />
        <!--#endif-->
        <!--#endif-->
        <!--#if (AddSharedToolkit)-->
        <!--#if (Net9)-->
        <PackageVersion Include="VijayAnand.Toolkit.Markup" Version="4.2.0" />
        <!--#elif (Net8)-->
        <PackageVersion Include="VijayAnand.Toolkit.Markup" Version="3.6.0" />
        <!--#else-->
        <PackageVersion Include="VijayAnand.Toolkit.Markup" Version="5.0.0" />
        <!--#endif-->
        <!--#endif-->
        <!--#if (AddMvvmToolkit || (Mvvm && !Razor))-->
        <PackageVersion Include="CommunityToolkit.Mvvm" Version="8.4.0" />
        <!--#endif-->
        <!--#if (CompiledBindings)-->
        <PackageVersion Include="CompiledBindings.MAUI" Version="1.0.18" />
        <!--#endif-->
