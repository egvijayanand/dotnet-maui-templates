        <PackageVersion Include="Reactor.Maui.Maps" Version="3.0.40" />
        <!--#endif-->
        <!--#elif (Net8)-->
        <PackageVersion Include="Reactor.Maui" Version="2.0.59" />
        <PackageVersion Include="Reactor.Maui.Canvas" Version="2.0.59" />
        <!--#if (AddMaps)-->
        <PackageVersion Include="Reactor.Maui.Maps" Version="2.0.59" />
        <!--#endif-->
        <!--#else-->
        <PackageVersion Include="Reactor.Maui" Version="3.0.40" />
        <PackageVersion Include="Reactor.Maui.Canvas" Version="3.0.40" />
