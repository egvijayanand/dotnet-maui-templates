        "Net8": {
            "type": "computed",
            "value": "(frameworkLower == \"net8.0\")"
        },
        "Net7OrEarlier": {
            "type": "computed",
            "value": "(frameworkLower == \"net6.0\" || frameworkLower == \"net7.0\")"
        },
        "Net7OrLater": {
            "type": "computed",
            "value": "(frameworkLower == \"net7.0\" || frameworkLower == \"net8.0\")"
        },
        "Net8OrLater": {
            "type": "computed",
            "value": "(frameworkLower == \"net8.0\")"
