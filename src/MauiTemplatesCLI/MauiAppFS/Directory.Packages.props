        <!--#elif (Net8)-->
        <PackageVersion Include="FSharp.Core" Version="8.0.403" />
        <PackageVersion Include="Microsoft.Extensions.Logging.Debug" Version="8.0.1" Condition="'$(Configuration)' == 'Debug'" />
        <!--#else-->
        <PackageVersion Include="FSharp.Core" Version="10.0.102" />
        <PackageVersion Include="Microsoft.Extensions.Logging.Debug" Version="10.0.2" Condition="'$(Configuration)' == 'Debug'" />
        <!--#endif-->
