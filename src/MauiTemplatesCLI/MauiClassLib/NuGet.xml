    <add key="maui-nightly" value="https://pkgs.dev.azure.com/dnceng/public/_packaging/dotnet10/nuget/v3/index.json" />
    <!--#elif (Net8)-->
    <add key="maui-nightly" value="https://aka.ms/maui-nightly/index.json" />
