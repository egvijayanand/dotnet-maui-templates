{
    "$schema": "https://json.schemastore.org/template",
    "author": "Vijay Anand E G",
    "defaultName": "MauiClassLib1",
    "classifications": [
        "MAUI",
        "Android",
        "iOS",
        "macOS",
        "Mac Catalyst",
        "Tizen",
        "WinUI",
        "Windows",
        "Library"
    ],
    "identity": "VijayAnand.MauiClassLib",
    "groupIdentity": "VijayAnand.MauiTemplates.Library",
    "description": "A project template for .NET MAUI Class Library",
    "name": ".NET MAUI Class Library",
    "shortName": "mauiclasslib",
    "sourceName": "MauiClassLib.1",
    "tags": {
        "language": "C#",
        "type": "solution",
        "vs_parametersToHide": "no-solution-file"
    },
    "guids": [
        "AE77891F-8A16-49C9-8887-1EF9692988CE",
        "1D17E9ED-22F1-4992-AE45-B8507EFD6950"
    ],
    "symbols": {
        "Framework": {
            "type": "parameter",
            "datatype": "choice",
            "description": "Option to choose the library target framework.",
            "choices": [
                {
                    "choice": "net9.0",
                    "description": "Target .NET 9",
                    "displayName": ".NET 9 (Preview)"
                },
                {
                    "choice": "net8.0",
                    "description": "Target .NET 8",
                    "displayName": ".NET 8 (LTS)"
                },
                {
                    "choice": "net7.0",
                    "description": "Target .NET 7",
                    "displayName": ".NET 7 (STS)"
                },
                {
                    "choice": "net6.0",
                    "description": "Target .NET 6",
                    "displayName": ".NET 6 (LTS)"
                }
            ],
            "replaces": "MAUI_TFM",
            "defaultValue": "net8.0"
        },
        "use-razor-sdk": {
            "type": "parameter",
            "datatype": "bool",
            "defaultValue": "false",
            "description": "Option to use .NET Razor SDK.",
            "displayName": "Option to use .NET _Razor SDK"
        },
        "use-maui-core": {
            "type": "parameter",
            "datatype": "bool",
            "defaultValue": "false",
            "description": "Option to use .NET MAUI Core (includes MAUI Essentials).",
            "displayName": "Option to use .NET MA_UI Core (includes MAUI Essentials)"
        },
        "use-maui-essentials": {
            "type": "parameter",
            "datatype": "bool",
            "defaultValue": "false",
            "description": "Option to use .NET MAUI Essentials.",
            "displayName": "Option to use .NET MAUI _Essentials"
        },
        "target-platform": {
            "type": "parameter",
            "datatype": "choice",
            "defaultValue": "All",
            "description": "Option to choose the application target platform.",
            "allowMultipleValues": true,
            "choices": [
                {
                    "choice": "All",
                    "description": "Targets all possible .NET MAUI supported platforms."
                },
                {
                    "choice": "Base",
                    "description": "Targets base framework (.NET 6/7/8)."
                },
                {
                    "choice": "Android",
                    "description": "Targets Android platform."
                },
                {
                    "choice": "iOS",
                    "description": "Targets iOS platform."
                },
                {
                    "choice": "macOS",
                    "description": "Targets macOS platform via Mac Catalyst."
                },
                {
                    "choice": "Windows",
                    "description": "Targets Windows platform via WinUI 3."
                },
                {
                    "choice": "Tizen",
                    "description": "Targets Tizen platform."
                },
                {
                    "choice": "Mobile",
                    "description": "Targets Android and iOS platforms."
                },
                {
                    "choice": "Desktop",
                    "description": "Targets Windows and macOS platforms."
                },
                {
                    "choice": "Apple",
                    "description": "Targets iOS and macOS platforms."
                }
            ]
        },
        "use-slnx": {
            "type": "parameter",
            "datatype": "bool",
            "defaultValue": "false",
            "description": "Option to use XML-based solution file (slnx).",
            "displayName": "Option to use XML-based solution file (sln_x)"
        },
        "central-pkg-mgmt": {
            "type": "parameter",
            "datatype": "bool",
            "defaultValue": "false",
            "description": "Option to configure NuGet Central Package Management.",
            "displayName": "Option to configure _NuGet Central Package Management"
        },
        "nightly": {
            "type": "parameter",
            "datatype": "bool",
            "defaultValue": "false",
            "description": "Option to use Nightly (CI) build.",
            "displayName": "Option to use Ni_ghtly (CI) build"
        },
        "include-toolkit": {
            "type": "parameter",
            "datatype": "bool",
            "defaultValue": "false",
            "description": "Option to add CommunityToolkit.Maui NuGet package reference.",
            "displayName": "Add Community_Toolkit.Maui NuGet package reference"
        },
        "include-syncfusion-toolkit": {
            "type": "parameter",
            "datatype": "bool",
            "defaultValue": "false",
            "description": "Option to add Syncfusion.Maui.Toolkit NuGet package reference.",
            "displayName": "Add _Syncfusion.Maui.Toolkit NuGet package reference"
        },
        "include-markup": {
            "type": "parameter",
            "datatype": "bool",
            "defaultValue": "false",
            "description": "Option to add CommunityToolkit.Maui.Markup NuGet package reference.",
            "displayName": "Add CommunityToolkit.Maui._Markup NuGet package reference"
        },
        "include-camera": {
            "type": "parameter",
            "datatype": "bool",
            "defaultValue": "false",
            "description": "Option to add CommunityToolkit.Maui.Camera NuGet package reference (.NET 8 or later).",
            "displayName": "Add CommunityTool_kit.Maui.Camera NuGet package reference"
        },
        "include-media-element": {
            "type": "parameter",
            "datatype": "bool",
            "defaultValue": "false",
            "description": "Option to add CommunityToolkit.Maui.MediaElement NuGet package reference (.NET 7 or later).",
            "displayName": "Add CommunityToolkit.Maui.Med_iaElement NuGet package reference"
        },
        "include-maps": {
            "type": "parameter",
            "datatype": "bool",
            "defaultValue": "false",
            "description": "Option to add Microsoft.Maui.Controls.Maps NuGet package reference (.NET 7 or later).",
            "displayName": "Add Microsoft.Maui.Controls.M_aps NuGet package reference"
        },
        "include-foldable": {
            "type": "parameter",
            "datatype": "bool",
            "defaultValue": "false",
            "description": "Option to add Microsoft.Maui.Controls.Foldable NuGet package reference (.NET 7 or later).",
            "displayName": "Add Microsoft.Maui.Controls.Fo_ldable NuGet package reference"
        },
        "include-mvvm-toolkit": {
            "type": "parameter",
            "datatype": "bool",
            "defaultValue": "false",
            "description": "Option to add CommunityToolkit.Mvvm (aka Microsoft MVVM Toolkit) NuGet package reference.",
            "displayName": "Add CommunityToolkit.Mvvm (aka Microsoft MV_VM Toolkit) NuGet package reference"
        },
        "include-shared-toolkit": {
            "type": "parameter",
            "datatype": "bool",
            "defaultValue": "false",
            "description": "Option to add VijayAnand.Toolkit.Markup NuGet package reference.",
            "displayName": "Add Vi_jayAnand.Toolkit.Markup (Shared Toolkit) NuGet package reference"
        },
        "conditional-compilation": {
            "type": "parameter",
            "datatype": "bool",
            "defaultValue": "false",
            "description": "Option to configure conditional compilation so that platform source files can be anywhere.",
            "displayName": "Option to configure c_onditional compilation for the project"
        },
        "no-solution-file": {
            "type": "parameter",
            "datatype": "bool",
            "defaultValue": "false",
            "description": "Option to skip creating a solution file."
        },
        "xmlEncodedName": {
            "type": "derived",
            "datatype": "string",
            "replaces": "SAFE-NAME",
            "valueSource": "name",
            "valueTransform": "xmlEncode"
        },
        "frameworkLower": {
            "type": "generated",
            "generator": "casing",
            "parameters": {
                "source": "Framework",
                "toLower": true
            }
        },
        "UseRazorSdk": {
            "type": "computed",
            "value": "(use-razor-sdk)"
        },
        "UseMaui": {
            "type": "computed",
            "value": "(!(use-maui-core || use-maui-essentials))"
        },
        "UseMauiCore": {
            "type": "computed",
            "value": "(use-maui-core)"
        },
        "UseMauiEssentials": {
            "type": "computed",
            "value": "(use-maui-essentials)"
        },
        "Net7": {
            "type": "computed",
            "value": "(frameworkLower == \"net7.0\")"
        },
        "Net8": {
            "type": "computed",
            "value": "(frameworkLower == \"net8.0\")"
        },
        "Net9": {
            "type": "computed",
            "value": "(frameworkLower == \"net9.0\")"
        },
        "Net7OrLater": {
            "type": "computed",
            "value": "(frameworkLower == \"net7.0\" || frameworkLower == \"net8.0\" || frameworkLower == \"net9.0\")"
        },
        "Net8OrLater": {
            "type": "computed",
            "value": "(frameworkLower == \"net8.0\" || frameworkLower == \"net9.0\")"
        },
        "AllPlatforms": {
            "type": "computed",
            "value": "(target-platform == \"All\")"
        },
        "IsAndroid": {
            "type": "computed",
            "value": "(target-platform == \"Android\" || target-platform == \"Mobile\")"
        },
        "IsiOS": {
            "type": "computed",
            "value": "(target-platform == \"Apple\" || target-platform == \"iOS\" || target-platform == \"Mobile\")"
        },
        "IsmacOS": {
            "type": "computed",
            "value": "(target-platform == \"Apple\" || target-platform == \"Desktop\" || target-platform == \"macOS\")"
        },
        "IsWindows": {
            "type": "computed",
            "value": "(target-platform == \"Desktop\" || target-platform == \"Windows\")"
        },
        "IsTizen": {
            "type": "computed",
            "value": "(target-platform == \"Tizen\")"
        },
        "IsBase": {
            "type": "computed",
            "value": "(target-platform == \"Base\")"
        },
        "FirstLine": {
            "type": "computed",
            "value": "(IsAndroid || IsiOS || IsmacOS)"
        },
        "MorePlatforms": {
            "type": "computed",
            "value": "(IsWindows || IsTizen || IsBase)"
        },
        "AddToolkit": {
            "type": "computed",
            "value": "(include-toolkit)"
        },
        "AddSyncfusionToolkit": {
            "type": "computed",
            "value": "(Net8OrLater && include-syncfusion-toolkit)"
        },
        "AddMarkup": {
            "type": "computed",
            "value": "(include-markup)"
        },
        "AddCamera": {
            "type": "computed",
            "value": "(Net8OrLater && include-camera)"
        },
        "AddMedia": {
            "type": "computed",
            "value": "(Net7OrLater && include-media-element)"
        },
        "AddMaps": {
            "type": "computed",
            "value": "(Net7OrLater && include-maps)"
        },
        "AddFoldable": {
            "type": "computed",
            "value": "(Net7OrLater && include-foldable)"
        },
        "AddMvvmToolkit": {
            "type": "computed",
            "value": "(include-mvvm-toolkit)"
        },
        "AddSharedToolkit": {
            "type": "computed",
            "value": "(include-shared-toolkit)"
        },
        "CentralPkgMgmt": {
            "type": "computed",
            "value": "(central-pkg-mgmt)"
        },
        "ConditionalCompilation": {
            "type": "computed",
            "value": "(conditional-compilation)"
        },
        "Nightly": {
            "type": "computed",
            "value": "(nightly)"
        },
        "PackageRef": {
            "type": "computed",
            "value": "(Net8OrLater || AddToolkit || AddMarkup || AddMaps || AddMvvmToolkit || AddSharedToolkit)"
        },
        "UseSlnx": {
            "type": "computed",
            "value": "(use-slnx)"
        },
        "SolnFile": {
            "type": "computed",
            "value": "(!no-solution-file)"
        },
        "HostIdentifier": {
            "type": "bind",
            "binding": "HostIdentifier"
        }
    },
    "primaryOutputs": [
        {
            "path": "Class1.cs"
        }
    ],
    "sources": [
        {
            "modifiers": [
                {
                    "condition": "(!SolnFile)",
                    "exclude": [
                        "MauiClassLib.1.code-workspace",
                        "MauiClassLib.1.sln",
                        "MauiClassLib.1.xml"
                    ]
                },
                {
                    "condition": "(UseSlnx)",
                    "exclude": [
                        "MauiClassLib.1.sln"
                    ],
                    "rename": {
                        "MauiClassLib.1.xml": "MauiClassLib.1.slnx"
                    }
                },
                {
                    "condition": "(!UseSlnx)",
                    "exclude": [
                        "MauiClassLib.1.xml"
                    ]
                },
                {
                    "condition": "(!Net8 && !CentralPkgMgmt)",
                    "exclude": [
                        "Directory.Packages.props"
                    ]
                },
                {
                    "condition": "(!(AllPlatforms || IsAndroid))",
                    "exclude": [
                        "Platforms/Android/**/*"
                    ]
                },
                {
                    "condition": "(!(AllPlatforms || IsiOS))",
                    "exclude": [
                        "Platforms/iOS/**/*"
                    ]
                },
                {
                    "condition": "(!(AllPlatforms || IsmacOS))",
                    "exclude": [
                        "Platforms/MacCatalyst/**/*"
                    ]
                },
                {
                    "condition": "(!(AllPlatforms || IsWindows))",
                    "exclude": [
                        "Platforms/Windows/**/*"
                    ]
                },
                {
                    "condition": "(!(AllPlatforms || IsTizen))",
                    "exclude": [
                        "Platforms/Tizen/**/*"
                    ]
                },
                {
                    "condition": "(UseMauiCore || UseMauiEssentials)",
                    "exclude": [
                        "Platforms/**/*"
                    ]
                },
                {
                    "condition": "(!(Net8 && Nightly))",
                    "exclude": [
                        "NuGet.config"
                    ]
                }
            ]
        }
    ]
}
