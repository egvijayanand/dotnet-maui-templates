{
    "$schema": "https://json.schemastore.org/template",
    "author": "Vijay Anand E G",
    "defaultName": "MauiItem1",
    "classifications": [
        "MAUI",
        "Android",
        "iOS",
        "macOS",
        "Windows",
        "Code"
    ],
    "identity": "VijayAnand.MauiItemCS",
    "groupIdentity": "VijayAnand.MauiTemplates.Item.Code",
    "description": "An item template for .NET MAUI Item in C#",
    "name": ".NET MAUI Item (C#)",
    "shortName": "maui-item-cs",
    "sourceName": "MauiItem.1",
    "primaryOutputs": [
        {
            "path": "MauiItem.1.cs"
        }
    ],
    "tags": {
        "language": "C#",
        "type": "item"
    },
    "symbols": {
        "namespace": {
            "type": "bind",
            "binding": "msbuild:RootNamespace",
            "replaces": "MyApp.Namespace",
            "defaultValue": "MyApp.Namespace"
        },
        "base": {
            "isRequired": true,
            "type": "parameter",
            "description": "Base type for the item.",
            "datatype": "text",
            "replaces": "ContentPage"
        },
        "generic": {
            "type": "parameter",
            "description": "Generic base type for the item.",
            "datatype": "text",
            "defaultValue": "",
            "replaces": "TObject"
        },
        "IsGeneric": {
            "type": "computed",
            "value": "(generic != \"\")"
        }
    },
    "constraints": {
        "dotnet7-sts": {
            "type": "sdk-version",
            "args": [
                "[7.0,)"
            ]
        },
        "csharp-only": {
            "type": "project-capability",
            "args": "CSharp"
        }
    }
}
