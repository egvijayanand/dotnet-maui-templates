<Project>
    <PropertyGroup>
        <ManagePackageVersionsCentrally>true</ManagePackageVersionsCentrally>
        <!--#if (Net8 && Nightly)-->
        <MauiVersion>8.0.4-nightly</MauiVersion>
        <!--#endif-->
    </PropertyGroup>
    <ItemGroup>
        <PackageVersion Include="Xamarin.Forms" Version="5.0.0.2622" />
        <!--#if (AddFormsEssentials)-->
        <PackageVersion Include="Xamarin.Essentials" Version="1.8.0" />
        <!--#endif-->
        <!--#if (AddFormsToolkit)-->
        <PackageVersion Include="Xamarin.CommunityToolkit" Version="2.0.6" />
        <!--#endif-->
        <!--#if (AddFormsMarkup)-->
        <PackageVersion Include="Xamarin.CommunityToolkit.Markup" Version="2.0.6" />
        <!--#endif-->
        <!--#if (Net8OrLater)-->
        <PackageVersion Include="Microsoft.Maui.Controls" Version="$(MauiVersion)" />
        <PackageVersion Include="Microsoft.Maui.Controls.Compatibility" Version="$(MauiVersion)" />
        <!--#if (AddMauiToolkit)-->
        <PackageVersion Include="CommunityToolkit.Maui" Version="7.0.0" />
        <!--#endif-->
        <!--#if (AddMauiMarkup)-->
        <PackageVersion Include="CommunityToolkit.Maui.Markup" Version="4.0.0" />
        <!--#endif-->
        <!--#if (AddSharedToolkit)-->
        <PackageVersion Include="VijayAnand.Toolkit.Markup" Version="3.1.0" />
        <!--#endif-->
        <!--#elif (Net7)-->
        <!--#if (AddMauiToolkit)-->
        <PackageVersion Include="CommunityToolkit.Maui" Version="6.1.0" />
        <!--#endif-->
        <!--#if (AddMauiMarkup)-->
        <PackageVersion Include="CommunityToolkit.Maui.Markup" Version="3.3.1" />
        <!--#endif-->
        <!--#if (AddSharedToolkit)-->
        <PackageVersion Include="VijayAnand.Toolkit.Markup" Version="2.1.0" />
        <!--#endif-->
        <!--#else-->
        <!--#if (AddMauiToolkit)-->
        <PackageVersion Include="CommunityToolkit.Maui" Version="2.0.0" />
        <!--#endif-->
        <!--#if (AddMauiMarkup)-->
        <PackageVersion Include="CommunityToolkit.Maui.Markup" Version="1.2.1" />
        <!--#endif-->
        <!--#if (AddSharedToolkit)-->
        <PackageVersion Include="VijayAnand.Toolkit.Markup" Version="1.1.0" />
        <!--#endif-->
        <!--#endif-->
        <!--#if (AddMvvmToolkit)-->
        <PackageVersion Include="CommunityToolkit.Mvvm" Version="8.2.2" />
        <!--#endif-->
    </ItemGroup>
</Project>
