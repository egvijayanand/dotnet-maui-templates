        <!--#if (AddSharedToolkit)-->
        <PackageVersion Include="VijayAnand.Toolkit.Markup" Version="1.2.0" />
        <!--#endif-->
        <!--#endif-->
        <!--#if (AddMauiMaps)-->
        <PackageVersion Include="Microsoft.Maui.Controls.Maps" Version="$(MauiVersion)" />
        <!--#endif-->
        <!--#if (AddMvvmToolkit)-->
        <PackageVersion Include="CommunityToolkit.Mvvm" Version="8.2.2" />
        <!--#endif-->
    </ItemGroup>
    <!--#endif-->
</Project>
